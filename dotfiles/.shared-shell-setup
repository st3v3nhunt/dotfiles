# Setup pyenv, rbenv
# eval "$(pyenv init -)"
# eval "$(rbenv init -)"

# set vim aliases based on what is available
if [[ -x "$(command -v vim.gtk)" ]]; then alias vim='vim.gtk'; fi

PATH=$PATH:$(npm -g bin)
export PATH

PATH=$PATH:$(go env GOPATH)/bin
export PATH
# Set GOPATH
GOPATH=$(go env GOPATH)
export GOPATH

# Source zsh-nvm
source ~/.zsh-nvm/zsh-nvm.plugin.zsh
